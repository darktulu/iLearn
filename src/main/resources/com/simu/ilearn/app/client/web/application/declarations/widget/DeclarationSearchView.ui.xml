<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'>

    <ui:with field="resource" type="com.simu.ilearn.common.client.resource.SharedResources"/>

    <g:HTMLPanel>
        <g:HTMLPanel ui:field="searchAxes"/>

        <div class="{resource.searchStyleCss.searchButtonContainer}">
            <b:Button ui:field="search" text="Rechercher" icon="SEARCH" type="PRIMARY"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>