<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="resource" type="com.simu.ilearn.common.client.resource.SharedResources"/>

    <g:HTMLPanel>
        <g:Label ui:field="label" addStyleNames="{resource.searchStyleCss.axeLabel}"/>
        <g:ListBox ui:field="value" addStyleNames="{resource.searchStyleCss.axeValue}"/>
    </g:HTMLPanel>
</ui:UiBinder>